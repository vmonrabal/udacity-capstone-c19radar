{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "update-contact",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 5,
      "maxLength": 50
    },
    "when": {
      "type": "string",
      "pattern": "[0-9]{4}-[0-9]{2}-[0-9]{2}"
    },
    "where": {
      "type": "string",
      "minLength": 5,
      "maxLength": 120
    },
    "infected": {
      "type": "boolean"
    }
  },
  "required": [
    "name",
    "when",
    "where",
    "infected"
  ],
  "additionalProperties": false
}
